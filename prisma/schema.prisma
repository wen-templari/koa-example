generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id       String  @id @map("user_id") @db.VarChar(50)
  password String? @map("user_password") @db.VarChar(50)
  name     String? @map("user_name") @db.VarChar(20)
  age      Int?    @map("user_age")
}
